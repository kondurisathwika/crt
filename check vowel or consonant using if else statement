import java.util.*;
class Vowelorconsonant
{
public static void main(String args[])
{
Scanner s=new Scanner(System.in);
char a;
a=s.next().charAt(0);
if(a=='a'||a=='A'||a=='e'||a=='E'||a=='i'||a=='I'||a=='o'||a=='O'||a=='u'||a=='U')
{
System.out.println("Vowel");
}
else
{
System.out.println("Consonant");
}
}
}
